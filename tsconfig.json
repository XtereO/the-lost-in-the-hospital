{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@ui/*": ["./src/ui/*"],
      "@core/*": ["./src/core/*"],
      "@assets/*": ["./src/assets/*"]
    }
  }
}
